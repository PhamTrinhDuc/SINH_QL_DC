#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int x , y , z , t ; cin >> x >> y >> z >> t  ; 
    long long  sum = x ; 
    sum += abs(y) + abs(z) + abs(t) ; 
    cout << sum ; 
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
