#include <bits/stdc++.h>
using namespace std;


int main() {
    int n ; cin >> n ; 
    int a[n] ;
    for(int &x : a ) cin >> x ; 
    int ok = 0 ; 
    for(int i=0 ; i<n ; i++ ){
        if(a[i]){
            cout << i ; ok =1  ; 
            break ; 
        }
    }
    if(!ok) cout << 0 ; 
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
