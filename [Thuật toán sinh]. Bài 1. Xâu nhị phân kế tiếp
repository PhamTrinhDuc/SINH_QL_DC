#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
string s ; 
void sinh(){
    int i = s.size() - 1 ; 
    while(i>=0 && s[i] == '1'){
        s[i]  = '0' ;
        i-- ; 
    }
    if(i!=-1) {
        s[i] = '1' ; 
    }
    cout << s ; 
}


int main() {
    cin >> s ;
    sinh() ; 
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
